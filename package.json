{
	"name": "example",
	"version": "0.0.1",
	"description": "",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"author": "Danny Creations",
	"license": "GPL-3.0",
	"private": true,
	"scripts": {
		"build": "rm -rf dist && tsc",
		"build:all": "npm run build --workspaces --if-present && npm run build",
		"start:pm2": "pm2 start . --stop-exit-codes 0",
		"start:dev": "npm run build && tsc-watch --onSuccess \"node .\"",
		"update:dep": "npx npm-check-updates -u -x got,chalk,@types/node",
		"update:dep:all": "npm run update:dep --workspaces --if-present && npm run update:dep"
	},
	"workspaces": [
		"packages/logger",
		"packages/core",
		"packages/mikro-orm",
		"packages/utilities"
	],
	"repository": "github:dannycreations/Shaka",
	"homepage": "https://github.com/dannycreations/Shaka#readme",
	"dependencies": {
		"chalk": "4.1.2",
		"dotenv": "^16.4.1",
		"got": "11.8.6",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.0",
		"nodemon": "^3.0.3",
		"ts-node": "^10.9.2",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.3.3"
	}
}
